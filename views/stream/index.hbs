        </li>
        {{/if}}
    </ul>

</nav>

<div class="table-responsive">
    <table class="table table-hover table-striped">
        <thead>
            <tr>
                <th scope="col">Action</th>
                <th scope="col">User</th>
                <th scope="col">Audit</th>
                <th scope="col">IP</th>
                <th scope="col">PGP key</th>
                <th scope="col">Time</th>

            </tr>
        </thead>
        <tbody>
            {{#each listing.data}}
            <tr>

                <td style="vertical-align: middle;">{{display.action.name}}</td>
                <td style="vertical-align: middle; cursor:default; "
                    title="{{display.owner.name}} [{{display.owner._id}}];" data-toggle="tooltip" data-placement="left">
                    {{display.owner.username}}</td>
                <td style="vertical-align: middle;">{{audit}}</td>
                <td style="vertical-align: middle;">{{display.ip}}</td>
                <td style="vertical-align: middle;"><span
                        title="{{display.keyData.name}} &lt;{{display.keyData.address}}&gt;" data-toggle="tooltip"
                        data-placement="left"
                        style="cursor:default; font-family: monospace;">{{display.keyData.fingerprint}}</span>
                </td>
                <td style="vertical-align: middle;" class="datetimestr" title="{{display.created}}"
                    data-toggle="tooltip" data-placement="left"></td>

            </tr>
            {{/each}}
        </tbody>
    </table>
</div>